{
    "env": {
        "node": true
    },
    "rules": {
       "strict": 0,
       "no-underscore-dangle": 0,
       "no-comma-dangle": 0,
       "curly": 0,
       "new-cap": 0,
       "no-use-before-define": 0,
       "no-shadow": 1,
       "quotes":  [1, "single"],
       "no-trailing-spaces": 1,
       "no-path-concat": 1,
       "semi": 1,
       "space-infix-ops": 1,
       "no-unused-vars": 1,
       "eol-last": 1,
       "no-undef": 1,
       "camelcase": 1,
       "key-spacing": 1,
       "handle-callback-err": 2
    },
    "globals":{
        "it": false,
        "describe": false,
        "module": false,
        "require": false,
        "before": false,
        "beforeEach": false,
        "after": false,
        "afterEach": false
    }
}
